96","usedDeprecatedRules":"48"},"C:\\curso-react\\hooks\\src\\index.js",[],["97","98"],"C:\\curso-react\\hooks\\src\\views\\App.jsx",[],"C:\\curso-react\\hooks\\src\\components\\layout\\Content.jsx",[],"C:\\curso-react\\hooks\\src\\components\\layout\\Menu.jsx",[],"C:\\curso-react\\hooks\\src\\views\\examples\\NotFound.jsx",[],"C:\\curso-react\\hooks\\src\\views\\examples\\Home.jsx",[],"C:\\curso-react\\hooks\\src\\views\\examples\\UseState.jsx",[],"C:\\curso-react\\hooks\\src\\views\\examples\\UseEffect.jsx",["99","100"],"import React, { useEffect, useState } from 'react'\nimport PageTitle from '../../components/layout/PageTitle'\nimport SectionTitle from '../../components/layout/SectionTitle'\n\n\nexport default (props) => {\n\n    const [number, setNumber] = useState(1);\n    const [fatorial, setFatorial] = useState(1);\n\n    const [numero, setNumero] = useState();\n    const [parOuImpar, setParOuImpar] = useState(\"Selecione um número.\");\n\n\n    function calcFatorial(number) {\n\n        if (number < 0) return \"Não existe fatorial de número negativo.\"\n\n        if (number === 0) return 1;\n\n        return number * calcFatorial(number - 1);\n\n    }\n\n    function calcParOuImpar(numero) {\n\n        let result = numero % 2;\n\n        if (result === 0) {\n            return \"Par.\";\n        } else {\n            return \"Ímpar.\";\n        }\n\n    }\n\n    useEffect(() => {                                      // userEffect é utilizado quando se quer que uma função seja acionada\n        // apenas quando a dependência tem alguma alteração. Quando [number]\n        setFatorial(calcFatorial(number))                  // sofrer alteração a função calcFatorial é acionada como um efeito \n    }, [number]);                                          // colateral da alteração de number.\n\n\n    useEffect(() => {\n        setParOuImpar(calcParOuImpar(numero))\n    }, [numero]);\n\n    return (\n        <div className=\"UseEffect\">\n            <PageTitle\n                title=\"Hook UseEffect\"\n                subtitle=\"Permite executar efeitos colaterais em componentes funcionais!\"\n            />\n            <SectionTitle title=\"Exercício 01\" />\n            <div>\n                <h1> Fatorial: {fatorial} </h1>\n                <input type=\"number\" className=\"input\"\n                    value={number}\n                    onChange={(result) => { setNumber(result.target.value) }} />\n            </div>\n            <SectionTitle title=\"Exercício 02\" />\n            <div>\n                <h1>Status: {parOuImpar}</h1>\n                <input className=\"input\" type=\"number\"\n                    onChange={(result) => { setNumero(result.target.value) }}></input>\n            </div>\n        </div>\n    )\n}\n\n","C:\\curso-react\\hooks\\src\\views\\examples\\UseRef.jsx",["101"],"import React, { useState, useRef, useEffect } from 'react'\nimport PageTitle from '../../components/layout/PageTitle'\nimport SectionTitle from '../../components/layout/SectionTitle'\n\nconst UseRef = (props) => {\n\n    const [value1, setValue1] = useState(\"\");\n    const [value2, setValue2] = useState(\"\");\n\n    const count = useRef(0);\n    const myinput1 = useRef(null);\n    const myinput2 = useRef(null);\n\n    useEffect(() => {\n        count.current++\n        myinput2.current.focus()\n    }, [value1]);\n\n    useEffect(() => {\n        count.current++\n        myinput1.current.focus()\n    }, [value2]);\n\n    \n    const merge = function(s1, s2){\n\n           return [...s1].map((element, index)=>`${element}${s2[index] || \"\"}`).join(\"\")\n    }\n    \n\n    return (\n        <div className=\"UseRef\">\n            <PageTitle\n                title=\"Hook UseRef\"\n                subtitle=\"Retorna um objeto mutável com a propriedade .current!\"\n            />\n            <SectionTitle title=\"Exercício 01\" />\n            <div className=\"center\">\n                <div>\n                    <h2 className=\"center\">Valor: {merge(value1, value2)} {count.current}</h2>\n                    <h2 className=\"center\">Valor:{count.current}</h2>\n\n                    <input ref={myinput1} value={value1} type=\"text\" className=\"input\" onChange={(result) => { setValue1(result.target.value) }} />\n                </div>\n            </div>\n            <SectionTitle title=\"Exercício 02\" />\n                <div>\n                    <h2 className=\"center\"></h2>\n                    <input ref={myinput2} value={value2} type=\"text\" className=\"input\" onChange={(result) => { setValue2(result.target.value) }} />\n                </div>\n        </div>\n    )\n}\n\nexport default UseRef\n","C:\\curso-react\\hooks\\src\\views\\examples\\UseCallback.jsx",[],"C:\\curso-react\\hooks\\src\\views\\examples\\UseContext.jsx",[],"C:\\curso-react\\hooks\\src\\views\\examples\\UseMemo.jsx",[],"C:\\curso-react\\hooks\\src\\views\\examples\\UseReducer.jsx",["102"],"C:\\curso-react\\hooks\\src\\views\\examples\\UseCustom.jsx",["103"],"import React from 'react'\nimport PageTitle from '../../components/layout/PageTitle'\nimport SectionTitle from '../../components/layout/SectionTitle'\n\nimport { useCounter } from \"../../hooks/useCounter\";\nimport { useFetch } from \"../../hooks/useFetch\";\n\nconst UseRef = (props) => {\n\n    const [count, inc, dec] = useCounter()\n    const url = 'http://files.cod3r.com.br/curso-react/estados.json'\n    const response = useFetch(url)\n\n    const showList = (data) => {\n\n        return data.map(item => {\n\n            <li>{item.nome} - {item.sigla}</li>\n        })\n    }\n\n    return (\n        <div className=\"UseCustom\">\n            <PageTitle\n                title=\"Seu Hook\"\n                subtitle=\"Vamos aprender como criar o nosso próprio Hook!\"\n            />\n            <SectionTitle title=\"Exercício 01\" />\n            <div className=\"center\">\n                <span className=\"text\">{count}</span>\n                <button className=\"btn\"\n                    onClick={() => { inc(count + 1) }}\n                >Incremento</button>\n                <button className=\"btn\"\n                    onClick={() => { dec(count - 1) }}\n                >Decremento</button>\n            </div>\n            <SectionTitle title=\"Exercício 02\" />\n                <div className=\"center\">\n                    <ul>\n                        {!response.loading ? showList(response.data) : 'Carregando...'}\n                    </ul>\n                </div>\n\n        </div>\n    )\n}\n\nexport default UseRef\n","C:\\curso-react\\hooks\\src\\components\\layout\\PageTitle.jsx",[],"C:\\curso-react\\hooks\\src\\components\\layout\\SectionTitle.jsx",[],"C:\\curso-react\\hooks\\src\\views\\examples\\UseCallBackButtoms.jsx",[],"C:\\curso-react\\hooks\\src\\data\\DataContext.js",[],"C:\\curso-react\\hooks\\src\\data\\Store.jsx",[],"C:\\curso-react\\hooks\\src\\store\\configExec02.js",[],["104","105"],"C:\\curso-react\\hooks\\src\\hooks\\useCounter.js",["106"],"import React, { useState } from 'react';\r\n\r\nexport const useCounter = (initialValue = 100) =>{\r\n    const [count, setCount] = useState(initialValue);\r\n\r\n    function inc(){\r\n        setCount(count + 1)\r\n    }\r\n\r\n    function dec(){\r\n        setCount(count - 1)\r\n    }\r\n\r\n    return [count, inc, dec]\r\n}","C:\\curso-react\\hooks\\src\\hooks\\useFetch.js",["107"],"import React, { useState, useEffect } from 'react';\r\nexport const useFetch = (url, method = 'get') => {\r\n\r\n    const [response, setResponse] = useState({\r\n        data: null,\r\n        loading: true\r\n    })\r\n\r\n    useEffect(function () {\r\n\r\n        fetch(url, { method })\r\n            .then(resp => resp.json())      // converteu pra json\r\n            .then(json => setResponse({     // json pronto ele muda estado do obj\r\n                data: json,\r\n                loading: false\r\n            }))\r\n\r\n    }, [url, method])\r\n\r\n    retur