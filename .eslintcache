]\n        setFatorial(calcFatorial(number))                  // sofrer alteração a função calcFatorial é acionada como um efeito \n    }, [number]);                                          // colateral da alteração de number.\n\n     \n    useEffect(()=> {\n        setParOuImpar(calcParOuImpar(numero))\n    },[numero]);\n\n    return (\n        <div className=\"UseEffect\">\n            <PageTitle\n                title=\"Hook UseEffect\"\n                subtitle=\"Permite executar efeitos colaterais em componentes funcionais!\"\n            />\n            <SectionTitle title=\"Exercício 01\" />\n                <div>\n                    <h1> Fatorial: {fatorial} </h1>\n                    <input type=\"number\" className=\"input\" value={number} onChange={(result) => { setNumber(result.target.value) }} />\n                </div>\n            <SectionTitle title=\"Exercício 02\"/>\n                <div>\n                    <h1>Status: {parOuImpar}</h1>\n                    <input className=\"input\" type=\"number\" onChange={(result)=>{ setNumero(result.target.value)} }></input>\n                </div>\n        </div>\n    )\n}\n\n","C:\\curso-react\\hooks\\src\\views\\examples\\UseRef.jsx",["72","73"],"C:\\curso-react\\hooks\\src\\views\\examples\\UseCallback.jsx",[],"C:\\curso-react\\hooks\\src\\views\\examples\\UseContext.jsx",[],"C:\\curso-react\\hooks\\src\\views\\examples\\UseMemo.jsx",[],"C:\\curso-react\\hooks\\src\\views\\examples\\UseReducer.jsx",[],"C:\\curso-react\\hooks\\src\\views\\examples\\UseCustom.jsx",[],"C:\\curso-react\\hooks\\src\\components\\layout\\PageTitle.jsx",[],"C:\\curso-react\\hooks\\src\\components\\layout\\SectionTitle.jsx",[],{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","severity":1,"message":"79","line":6,"column":1,"nodeType":"80","endLine":65,"endColumn":2},{"ruleId":"81","severity":1,"message":"82","line":40,"column":8,"nodeType":"83","endLine":40,"endColumn":16,"suggestions":"84"},{"r