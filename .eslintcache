eWarningCount":0,"source":"95","usedDeprecatedRules":"96"},"C:\\curso-react\\hooks\\src\\index.js",[],["97","98"],"C:\\curso-react\\hooks\\src\\views\\App.jsx",[],"C:\\curso-react\\hooks\\src\\components\\layout\\Content.jsx",[],"C:\\curso-react\\hooks\\src\\components\\layout\\Menu.jsx",[],"C:\\curso-react\\hooks\\src\\views\\examples\\NotFound.jsx",[],"C:\\curso-react\\hooks\\src\\views\\examples\\Home.jsx",[],"C:\\curso-react\\hooks\\src\\views\\examples\\UseState.jsx",[],"C:\\curso-react\\hooks\\src\\views\\examples\\UseEffect.jsx",["99","100"],"import React, { useEffect, useState } from 'react'\nimport PageTitle from '../../components/layout/PageTitle'\nimport SectionTitle from '../../components/layout/SectionTitle'\n\n\nexport default (props) => {\n\n    const [number, setNumber] = useState(1);\n    const [fatorial, setFatorial] = useState(1);\n\n    const [numero, setNumero] = useState();\n    const [parOuImpar, setParOuImpar] = useState(\"Selecione um número.\");\n\n\n    function calcFatorial(number) {\n\n        if (number < 0) return \"Não existe fatorial de número negativo.\"\n\n        if (number === 0) return 1;\n\n        return number * calcFatorial(number - 1);\n\n    }\n\n    function calcParOuImpar(numero) {\n\n        let result = numero % 2;\n\n        if (result === 0) {\n            return \"Par.\";\n        } else {\n            return \"Ímpar.\";\n        }\n\n    }\n\n    useEffect(() => {                                      // userEffect é utilizado quando se quer que uma função seja acionada\n        // apenas quando a dependência tem alguma alteração. Quando [number]\n        setFatorial(calcFatorial(number))                  // sofrer alteração a função calcFatorial é acionada como um efeito \n    }, [number]);                                          // colateral da alteração de number.\n\n\n    useEffect(() => {\n        setParOuImpar(calcParOuImpar(numero))\n    }, [numero]);\n\n    return (\n        <div className=\"UseEffect\">\n            <PageTitle\n                title=\"Hook UseEffect\"\n                subtitle=\"Permite executar efeitos colaterais em componentes funcionais!\"\n            />\n            <SectionTitle title=\"Exercício 01\" />\n            <div>\n                <h1> Fatorial: {fatorial} </h1>\n                <input type=\"number\" className=\"input\"\n                    value={number}\n                    onChange={(result) => { setNumber(result.target.value) }} />\n            </div>\n            <SectionTitle title=\"Exercício 02\" />\n            <div>\n                <h1>Status: {parOuImpar}</h1>\n                <input className=\"input\" type=\"number\"\n                    onChange={(result) => { setNumero(result.target.value) }}></input>\n            </div>\n        </div>\n    )\n}\n\n","C:\\curso-react\\hooks\\src\\views\\examples\\UseRef.jsx",["101"],"import React, { useState, useRef, useEffect } from 'react'\nimport PageTitle from '../../components/layout/PageTitle'\nimport SectionTitle from '../../components/layout/SectionTitle'\n\nconst UseRef = (props) => {\n\n    const [value1, setValue1] = useState(\"\");\n    const [value2, setValue2] = useState(\"\");\n\n    const count = useRef(0);\n    const myinput1 = useRef(null);\n    const myinput2 = useRef(null);\n\n    useEffect(() => {\n        count.current++\n        myinput2.current.focus()\n    }, [value1]);\n\n    useEffect(() => {\n        count.current++\n        myinput1.current.focus()\n    }, [value2]);\n\n    \n    const merge = function(s1, s2){\n\n           return [...s1].map((element, index)=>`${element}${s2[index] || \"\"}`).join(\"\")\n    }\n    \n\n    return (\n        <div className=\"UseRef\">\n            <PageTitle\n                title=\"Hook UseRef\"\n                subtitle=\"Retorna um objeto mutável com a propriedade .current!\"\n            />\n            <SectionTitle title=\"Exercício 01\" />\n            <div className=\"center\">\n                <div>\n                    <h2 className=\"center\">Valor: {merge(value1, value2)} {count.current}</h2>\n                    <h2 className=\"center\">Valor:{count.current}</h2>\n\n                    <input ref={myinput1} value={value1} type=\"text\" className=\"input\" onChange={(result) => { setValue1(result.target.value) }} />\n                </div>\n            </div>\n            <SectionTitle title=\"Exercício 02\" />\n                <div>\n                    <h2 className=\"center\"></h2>\n                    <input ref={myinput2} value={value2} type=\"text\" className=\"input\" onChange={(result) => { setValue2(result.target.value) }} />\n                </div>\n        </div>\n    )\n}\n\nexport default UseRef\n","C:\\curso-react\\hooks\\src\\views\\examples\\UseCallback.jsx",[],"C:\\curso-react\\hooks\\src\\views\\examples\\UseContext.jsx",[],"C:\\curso-react\\hooks\\src\\views\\examples\\UseMemo.jsx",[],"C:\\curso-react\\hooks\\src\\views\\examples\\UseReducer.jsx",["102"],"import React, { useReducer } from 'react'\nimport PageTitle from '../../components/layout/PageTitle'\nimport SectionTitle from '../../components/layout/SectionTitle'\nimport {initialState2, reducer2  } from \"../../store/configExec02\";\n\nconst initialState = {\n\n    cart: [],\n    products: [],\n    user: null,\n    number: 0\n}\n\nfunction reducer(previousState, action) {       // Função tá recebendo o estado atual da variável/objeto/array...\n                                                // e as ações dos eventos dos botões para determinar o TIPO DA AÇÃO  \n                                                // e como alterará o estado atual da variável/obj/array...\n                                                // OBS: action é um obj que pode possuir parâmetros e neste caso\n                                                // recebe o parâmetro type e também o payload.\n    switch (action.type) {\n\n        case 'numberAdd2':\n            return { ...previousState, number: previousState.number + 2 }\n\n        case 'login':\n            return { ...previousState, user: { name: action.payload } }\n\n        default:\n            return previousState\n    }\n}\n\n\n\nconst UseReducer = (props) => {\n\n    /* Declara o estado inicial do parâmetro, a função de disparo\n       da ação (dispatch) e o hook useReducer recebe a função  \n       (reducer) que vai evoluir o estado e o estado inicial (initialState) */\n    const [state, dispatch] = useReducer(reducer, initialState)\n\n    /* Portanto quando o evento é detectado o parâmetro type do obj\n       é passado ao dispatch que tranfere para a função reducer e o\n       estado da variável é alterado e atualizado */\n\n\n    /* Exercício 02 */\n    const [state2, dispatch2] = useReducer(reducer2, initialState2)\n    \n\n    return (\n        <div className=\"UseReducer\">\n            <PageTitle\n                title=\"Hook UseReducer\"\n                subtitle=\"Uma outra forma de ter estado em componentes funcionais!\"\n            />\n\n            <SectionTitle title=\"Exercício 01\" />\n            <div className=\"center\">\n                {state.user ?\n                    <span className=\"text\">{state.user.name}</span> :\n                    <span className=\"text\">Sem usuário</span>}\n\n                <span className=\"text\">{state.number}</span>\n            </div>\n            <div className=\"center\">\n                <h2 >\n                    <button className=\"btn\"\n                        onClick={() => { dispatch({ type: 'numberAdd2' }) }}>+2</button>\n                    <button className=\"btn\"\n                        onClick={() => { dispatch({ type: 'login', payload: 'Joana' }) }}>Login\n                </button>\n                </h2>\n            </div>\n\n            <SectionTitle title=\"Exercício 02\" />\n            <div className=\"center\">\n                <span className=\"text\">{state2.number}</span>\n                <h2>\n                    <button className=\"btn\"\n                        onClick={() => { dispatch2({ type: 'multiple_7' }) }}> *7</button>\n                    <button className=\"btn\"\n                        onClick={() => { dispatch2({ type: 'divide_25' }) }}> /25</button>\n                    <button className=\"btn\"\n                        onClick={() => { dispatch2({ type: 'numberInt' }) }}> Int</button>\n                </h2>\n                <div className=\"center\">\n                    <h2>\n                        <span className=\"text\">Número: </span>\n                        <input type=\"text\"\n                            className=\"input\"\n                            type=\"number\"\n                            value={state2.number}\n                            onChange={(result)=>{dispatch2({type:'add_number', payload: result.target.value})}}\n                        />\n                    </h2>\n                </div>\n            </div>\n\n        </div>\n    )\n}\n\nexport default UseReducer\n","C:\\curso-react\\hooks\\src\\views\\examples\\UseCustom.jsx",["103"],"C:\\curso-react\\hooks\\src\\components\\layout\\PageTitle.jsx",[],"C:\\curso-react\\hooks\\src\\components\\layout\\SectionTitle.jsx",[],"C:\\curso-react\\hooks\\src\\views\\examples\\UseCallBackButtoms.jsx",[],"C:\\curso-react\\hooks\\src\\data\\DataContext.js",[],"C:\\curso-react\\hooks\\src\\data\\Store.jsx",[],"C:\\curso-react\\hooks\\src\\store\\configExec02.js",[],"C:\\curso-react\\hooks\\src\\hooks\\useCounter.js",["104"],"import React, { useState } from 'react';\r\n\r\nexport const useCounter = (initialValue = 100) =>{\r\n    const [count, setCount] = useState(initialValue);\r\n\r\n    function inc(){\r\n        setCount(count + 1)\r\n    }\r\n\r\n    function dec(){\r\n        setCount(count - 1)\r\n    }\r\n\r\n    return [count, inc, dec]\r\n}","C:\\curso-react\\hooks\\src\\hooks\\useFetch.js",["105"],"import React, { useState, useEffect } from 'react';\r\nexport const useFetch = (url, method = 'get') => {\r\n\r\n    const [response, setResponse] = useState({\r\n        data: null,\r\n        loading: true\r\n    })\r\n\r\n    useEffect(function () {\r\n\r\n        fetch(url, { method })\r\n            .then(resp => resp.json())      // converteu pra json\r\n            .then(json => setResponse({     // json pronto ele muda estado do obj\r\n                data: json,\r\n                loading: false\r\n            }))\r\n\r\n    }, [url, method])\r\n\r\n    return response\r\n}",["106","107"],{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","replacedBy":"111"},{"ruleId":"112","severity":1,"message":"113","line":6,"column":1,"nodeType":"114","endLine":68,"endColumn":2},{"ruleId":"115","severity":1,"message":"116","line":40,"column":8,"nodeType":"117","endLine":40,"endColumn":16,"suggestions":"118"},{"ruleId":"119","severity":1,"message":"120","line":48,"column":21,"nodeType":"121","endLine":48,"endColumn":44},{"ruleId":"122","severity":1,"message":"123","line":91,"column":29,"nodeType":"124","endLine":91,"endColumn":42},{"ruleId":"125","severity":1,"message":"126","line":16,"column":30,"nodeType":"127","messageId":"128","endLine":16,"endColumn":32},{"ruleId":"129","severity":1,"message":"130","line":1,"column":8,"nodeType":"131","messageId":"132","endLine":1,"endColumn":13},{"ruleId":"129","severity":1,"message":"130","line":1,"column":8,"nodeType":"131","messageId":"132","endLine":1,"endColumn":13},{"ruleId":"108","replacedBy":"133"},{"ruleId":"110","replacedBy":"134"},"no-native-reassign",["135"],"no-negated-in-lhs",["136"],"import/no-anonymous-default-export","Assign arrow function to a variable before exporting as module default","ExportDefaultDeclaration","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'calcFatorial'. Either include it or remove the dependency array.","ArrayExpression",["137"],"jsx-a11y/heading-has-content","Headings must have content and the content must be accessible by a screen reader.","JSXOpeningElement","reac